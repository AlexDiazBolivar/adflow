# $Id: Makefile 1191 2006-06-19 09:07:42Z llh $

FC=pgf77 -g
CC=gcc -g -Werror -Wconversion

all : testMemSize adStack.o adBuffer.o profile.o dpStack.o dpTest.o ddTest.o validityTest.o

profile.o: profile.c
	$(CC) -c profile.c

testMemSizef.o : testMemSizef.f
	$(FC) -c testMemSizef.f

testMemSizec.o : testMemSizec.c
	$(CC) -c testMemSizec.c

testMemSize : testMemSizef.o testMemSizec.o
	$(FC) testMemSizef.o testMemSizec.o -o testMemSize

adBuffer.o : adBuffer.f
	$(FC) -c adBuffer.f

adStack.o : adStack.c
	$(CC) -c adStack.c

dpStack.o : dpStack.c
	$(CC) -c dpStack.c

dpTest.o : dpTest.f
	$(FC) -c dpTest.f

validityTest.o : validityTest.f
	$(FC) -c validityTest.f

clean :
	-rm -f .*~ *.o testMemSize
